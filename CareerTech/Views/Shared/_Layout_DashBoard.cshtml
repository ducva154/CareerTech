<!DOCTYPE html>
<html lang="en">
<head>
    <title>Alpha | Responsive Admin Dashboard Template</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta charset="UTF-8">
    <meta name="description" content="Responsive Admin Dashboard Template" />
    <meta name="keywords" content="admin,dashboard" />
    <meta name="author" content="Steelcoders" />
    <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">

    <!-- Styles -->
    <link type="text/css" rel="stylesheet" href="~/Content/Dashboard/assets/plugins/materialize/css/materialize.min.css" />
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/Content/Dashboard/assets/plugins/metrojs/MetroJs.min.css" rel="stylesheet">
    <link href="~/Content/Dashboard/assets/plugins/weather-icons-master/css/weather-icons.min.css" rel="stylesheet">


    <!-- Theme Styles -->
    <link href="~/Content/Dashboard/assets/css/alpha.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard/assets/css/custom.css" rel="stylesheet" type="text/css" />

    <!-- Script -->
    <script src="~/Content/Dashboard/assets/plugins/ckeditor/ckeditor.js"></script>

</head>
<body>
    <div class="mn-content fixed-sidebar">
        <header class="mn-header navbar-fixed">
            <nav class="cyan darken-1">
                <div class="nav-wrapper row">
                    <section class="material-design-hamburger navigation-toggle">
                        <a href="javascript:void(0)" data-activates="slide-out" class="button-collapse show-on-large material-design-hamburger__icon">
                            <span class="material-design-hamburger__layer"></span>
                        </a>
                    </section>
                    <div class="header-title col s3 m3">
                        <span class="chapter-title">Parner</span>
                    </div>
                </div>
            </nav>
        </header>
        <aside id="slide-out" class="side-nav white fixed">
            <div class="side-nav-wrapper" style="background-color: #F3F3F3;">
                <div class="sidebar-profile">
                    <div class="sidebar-profile-image">
                        <img src="~/Content/Dashboard/assets/images/profile-image.png" class="circle" alt="">
                    </div>
                    <div class="sidebar-profile-info">
                        <p>David Doe</p>
                        <span>david@gmail.com</span>
                    </div>
                </div>
                <ul class="sidebar-menu collapsible collapsible-accordion" data-collapsible="accordion" style="padding-top: 0px!important;">
                    @*---------------------------------------------------Add view-------------------------------------------------*@

                    @*@if (true)
                        {
                            @Html.Partial("_LoginPartial")
                        }
                        else if(true)
                        {
                            @Html.Partial("_LoginPartial")
                        }
                        else
                        {
                            @Html.Partial("_LoginPartial")
                        };*@
                    @Html.Partial("_Partner_Menu_Partial")
                    @*@Html.Partial("_LoginPartial")
                    @Html.Partial("_LoginPartial")*@

                </ul>
            </div>
        </aside>
        <main class="mn-inner inner-active-sidebar">
            <div class="middle-content">


                @RenderBody()



            </div>
        </main>
    </div>
    <div class="left-sidebar-hover"></div>
    <!-- Javascripts -->
    <script src="~/Content/Dashboard/assets/plugins/jquery/jquery-2.2.0.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/materialize/js/materialize.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/material-preloader/js/materialPreloader.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/jquery-blockui/jquery.blockui.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/waypoints/jquery.waypoints.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/counter-up-master/jquery.counterup.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/jquery-sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/chart.js/chart.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/flot/jquery.flot.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/flot/jquery.flot.time.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/flot/jquery.flot.symbol.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/flot/jquery.flot.resize.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/flot/jquery.flot.tooltip.min.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/curvedlines/curvedLines.js"></script>
    <script src="~/Content/Dashboard/assets/plugins/peity/jquery.peity.min.js"></script>
    <script src="~/Content/Dashboard/assets/js/alpha.min.js"></script>
    <script src="~/Content/Dashboard/assets/js/pages/dashboard.js"></script>
</body>
</html>
